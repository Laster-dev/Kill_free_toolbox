        <Grid>
            <!-- 主界面内容 -->
            <Grid x:Name="MainGrid" RenderTransformOrigin="0.5,0.5">
                <Grid.RenderTransform>
                    <ScaleTransform/>
                </Grid.RenderTransform>
                <Grid.RowDefinitions>
                    <RowDefinition Height="31"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <!-- 标题栏 -->
                <Grid Height="31"  Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="{StaticResource appico}" Width="20" Height="20" Stretch="Uniform" Margin="10,0"/>


                        <TextBlock x:Name="textblock_title" Grid.Column="1" Text="免杀工具箱" Foreground="{DynamicResource PrimaryForeground}" VerticalAlignment="Center" Margin="5,0,0,0" FontSize="14" HorizontalAlignment="Left"/>

                    </StackPanel>
                    <Button Grid.Column="3" Width="25" Height="25" Margin="0,0,3,0" Click="MinimizeButton_Click" Background="#FFD700" Tag="#FFEC8B" BorderBrush="#DAA520" Style="{DynamicResource ControlButtonStyle}">
                        <Canvas>
                            <Line X1="6" Y1="10" X2="14" Y2="10" Stroke="Black" StrokeThickness="2"/>
                        </Canvas>
                    </Button>
                    <Button Grid.Column="4" Width="25" Height="25" Margin="0,0,3,0" Click="Button_Click" Background="#E57373" Tag="#EF9A9A" BorderBrush="#D32F2F" Style="{DynamicResource ControlButtonStyle}">
                        <Canvas>
                            <Line X1="6" Y1="6" X2="14" Y2="14" Stroke="Black" StrokeThickness="2"/>
                            <Line X1="14" Y1="6" X2="6" Y2="14" Stroke="Black" StrokeThickness="2"/>
                        </Canvas>
                    </Button>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="170"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <!-- 现代化侧边菜单面板 -->
                    <StackPanel Margin="0,10,0,0" Cursor="Hand">
                        <!-- 菜单项 -->
                        <RadioButton x:Name="btnStringObf" GroupName="MenuGroup" IsChecked="True">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource stringobf}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="文本加密" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>

                        <RadioButton     x:Name="btnBin2head"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource bin}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="文件编码" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnPowershellObf"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource powershell}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="ps1混淆" Margin="12,0,0,0" FontWeight="Bold"/>
                                <!--<TextBlock Text="加载器生成" Margin="12,0,0,0" FontWeight="Bold"/>-->
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnPE2shellcode"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource PE2shellcode}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="PE-&gt;SC" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnloader"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource loader}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="加载器生成" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnbaijiahei"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource dll}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="白加黑生成" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btndefendercheck"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource defender}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="DefCheck" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnRubbish"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource Rubbish}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="垃圾生成" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnNetwork"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource Network}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="网络调试" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton     x:Name="btnUpdate"     GroupName="MenuGroup">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource update}" Width="18" Height="18" Stretch="Uniform" />
                                <TextBlock Text="更新内容" Margin="12,0,0,0" FontWeight="Bold"/>
                            </StackPanel>
                        </RadioButton>

                    </StackPanel>

                    <Grid Height="566" VerticalAlignment="Top">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <!--商标版权许可-->
                        <TextBlock Grid.Row="1" TextWrapping="Wrap" Margin="0,0,0,8"
           FontSize="12" Foreground="{DynamicResource DisabledForeground}" 
           TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Bottom">
           
    @Laster
                        </TextBlock>

                    </Grid>
                    <!-- 内容区域 -->
                    <Grid Grid.Column="1" x:Name="MainFram"/>
                </Grid>
            </Grid>
        </Grid>
    </Border>

</Window>
