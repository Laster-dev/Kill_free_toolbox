<Page x:Class="Kill_free_toolbox.Views.Pages.Page_String_Obf"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kill_free_toolbox.Views.Pages"
      mc:Ignorable="d"   d:DesignHeight="450" d:DesignWidth="800"
      TextOptions.TextFormattingMode="Display"
      Title="Page_String_Obf">
    <Grid>
        <TabControl>
            <TabItem Header="C#" FontSize="16">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="csstr" Height="32" Margin="5" TextChanged="str_TextChanged"/>
                    </Grid>
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="AUTO"/>
                        </Grid.RowDefinitions>
                        <TextBox x:Name="csstrout" Text="" Margin="5" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible"/>
                        <Grid Grid.Row="1">
                            <Button Content="复制" Margin="5" Padding="10,5" Click="Button_Copy_Click"/>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="C/CPP">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                     
                        <TextBox x:Name="cstr" Height="32" Margin="5" TextChanged="cstr_TextChanged"/>
                        <CheckBox x:Name="checkbox_is_wchar_t" Content="使用 wchar_t" Margin="5" VerticalAlignment="Center" Grid.Column="1" Checked="checkbox_is_wchar_t_Checked" Unchecked="checkbox_is_wchar_t_Unchecked"/>
                    </Grid>
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="AUTO"/>
                        </Grid.RowDefinitions>
                        <TextBox x:Name="cstrout" Text="" Margin="5" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible"/>
                        <Grid Grid.Row="1">
                            <Button Content="复制" Margin="5" Padding="10,5" Click="cButton_Copy_Click"/>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Python" FontSize="16">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="pystr" Height="32" Margin="5" TextChanged="pystr_TextChanged"/>
                    </Grid>
                    <Grid Grid.Row="1">
