                        </Grid.RowDefinitions>
                        <TextBox x:Name="varstrout" Text="" Margin="5" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible"/>
                        <Grid Grid.Row="1">
                            <Button Content="复制" Margin="5" Padding="10,5" Click="varButton_Copy_Click"/>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
